    "postinstall": "husky install"
  },
  "standard-version": {
    "scripts": {
      "postbump": "yarn update-version && yarn build",
      "precommit": "git add libs/ngx-mapbox-gl/package.json"
    }
  },
  "lint-staged": {
    "*": [
      "nx affected:lint --fix --files",
      "nx format:write --files"
    ]
